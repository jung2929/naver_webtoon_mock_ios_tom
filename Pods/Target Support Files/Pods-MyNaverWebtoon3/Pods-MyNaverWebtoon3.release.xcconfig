ALWAYS_EMBED_SWIFT_STANDARD_LIBRARIES = YES
FRAMEWORK_SEARCH_PATHS = $(inherited) "${PODS_CONFIGURATION_BUILD_DIR}/AMScrollingNavbar" "${PODS_CONFIGURATION_BUILD_DIR}/ScrollingFollowView" "${PODS_CONFIGURATION_BUILD_DIR}/SegementSlide"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_CONFIGURATION_BUILD_DIR}/AMScrollingNavbar/AMScrollingNavbar.framework/Headers" "${PODS_CONFIGURATION_BUILD_DIR}/ScrollingFollowView/ScrollingFollowView.framework/Headers" "${PODS_CONFIGURATION_BUILD_DIR}/SegementSlide/SegementSlide.framework/Headers"
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_LDFLAGS = $(inherited) -ObjC -framework "AMScrollingNavbar" -framework "ScrollingFollowView" -framework "SegementSlide" -framework "UIKit"
OTHER_SWIFT_FLAGS = $(inherited) -D COCOAPODS
PODS_BUILD_DIR = ${BUILD_DIR}
PODS_CONFIGURATION_BUILD_DIR = ${PODS_BUILD_DIR}/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_PODFILE_DIR_PATH = ${SRCROOT}/.
PODS_ROOT = ${SRCROOT}/Pods
